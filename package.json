{
    "name": "gatsby-plugin-template-typescript",
    "version": "1.0.0",
    "description": "a starter repo for creating gatsby plugins in typescript",
    "main": "gatsby-node.js",
    "scripts": {
        "build": "tsc",
        "prepare": "tsc",
        "watch": "tsc -w",
        "lint": "eslint --ignore-path .gitignore \"**/*.{ts,tsx,js}\"",
        "lint:fix": "eslint --ignore-path .gitignore \"**/*.{ts,tsx,js}\" --fix"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/glweems/gatsby-plugin-template-typescript"
    },
    "bugs": {
        "url": "https://github.com/glweems/gatsby-plugin-template-typescript/issues"
    },
    "homepage": "https://github.com/glweems/gatsby-plugin-template-typescript.git",
    "keywords": [
        "gatsby",
        "gatsby-plugin",
        "gatsby-source-plugin",
        "behance"
    ],
    "author": "Garrett Weems <gwgraphicdesign@gmail.com>",
    "license": "MIT",
    "peerDependencies": {
        "gatsby": ">=2"
    },
    "devDependencies": {
        "@types/node": "^14.0.23",
        "@typescript-eslint/eslint-plugin": "^3.6.1",
        "@typescript-eslint/parser": "^3.6.1",
        "eslint": "^7.4.0",
        "eslint-plugin-react": "^7.20.3",
        "gatsby": "^4.25.7",
        "typescript": "^3.9.6"
    },
    "dependencies": {
        "@types/react-dom": "^16.9.8"
    }
}
